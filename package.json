    "@uirouter/angularjs": "^1.0.7",
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-aria": "^1.6.4",
    "angular-masonry": "^0.17.0",
    "angular-materialize": "^0.2.2",
