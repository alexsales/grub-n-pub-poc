{
  "homepage": "https://alexsales.github.io/grub-n-pub-poc/",
  "name": "grub-n-pub",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "description": "Never eat alone again! An app to help you find others to dine with and chat about random stuff; no strings attached, just good company.",
  "main": "index.js",
  "watch": {
    "transpileAndBundle": "./{src, spec}/*.js"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
    "babelBrowseCtrl": "babel ./src/app/appAngular.js -o ./src/app/appAngular-transpiled.js",
    "webpackBrowseCtrl": "webpack ./src/app/appAngular-transpiled.js ./src/app/appAngular-bundle.js",
    "transpileAndBundle": "npm run babelBrowseCtrl && npm run webpackBrowseCtrl",
    "watch": "npm-watch"
  },
  "keywords": [
    "Googla",
    "Maps",
    "Firebase",
    "AngularFire",
    "Materialize",
    "MaterializeCSS",
    "Bootstrap",
    "AngularJS",
    "Yelp",
    "food",
    "entertainment",
    "nightlife",
    "dating",
    "lunch",
    "eateries",
    "restaurants",
    "dining"
  ],
  "author": "Alex Sales <alexsales@sbcglobal.net",
  "license": "MIT",
